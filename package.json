{
  "name": "sy-devs-cms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "test": "pnpm run test:int",
    "test:e2e": "cross-env NODE_OPTIONS=--no-deprecation pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types && wrangler types",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "deploy:prod": "pnpm run deploy:database && pnpm run deploy:app",
    "deploy:database": "cross-env NODE_ENV=production CLOUDFLARE_ENV= PAYLOAD_SECRET=ignore payload migrate && wrangler d1 execute sahajcloud --command 'PRAGMA optimize' --remote",
    "deploy:app": "wrangler deploy",
    "db:create": "wrangler d1 create",
    "db:migrations:create": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:create",
    "db:schema": "wrangler d1 execute sahajcloud --command '.schema' --remote"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.919.0",
    "@aws-sdk/lib-storage": "^3.919.0",
    "@nouance/payload-better-fields-plugin": "^2.0.2",
    "@opennextjs/cloudflare": "^1.13.1",
    "@payloadcms/db-d1-sqlite": "^3.64.0",
    "@payloadcms/db-sqlite": "^3.64.0",
    "@payloadcms/email-nodemailer": "^3.64.0",
    "@payloadcms/graphql": "3.64.0",
    "@payloadcms/next": "^3.64.0",
    "@payloadcms/payload-cloud": "^3.64.0",
    "@payloadcms/plugin-form-builder": "^3.64.0",
    "@payloadcms/plugin-seo": "^3.64.0",
    "@payloadcms/richtext-lexical": "^3.64.0",
    "@payloadcms/storage-s3": "^3.64.0",
    "@payloadcms/ui": "^3.64.0",
    "@sentry/cloudflare": "^10.27.0",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/styled-components": "^5.1.35",
    "aws-crt": "^1.27.5",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "graphql": "^16.11.0",
    "next": "15.3.2",
    "payload": "^3.64.0",
    "react": "19.1.0",
    "resend": "^6.5.2",
    "slugify": "^1.6.6",
    "styled-components": "^6.1.19",
    "tmp": "^0.2.5",
    "wrangler": "^4.49.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251120.0",
    "@payloadcms/plugin-cloud-storage": "^3.64.0",
    "@playwright/test": "1.50.0",
    "@testing-library/react": "16.3.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.18.13",
    "@types/nodemailer": "^6.4.21",
    "@types/pg": "^8.15.6",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "@types/tmp": "^0.2.6",
    "@vitejs/plugin-react": "4.5.2",
    "better-sqlite3": "^12.4.6",
    "eslint": "^9.38.0",
    "eslint-config-next": "15.3.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jsdom": "26.1.0",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3",
    "playwright": "1.50.0",
    "playwright-core": "1.50.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ],
    "peerDependencyRules": {
      "ignoreMissing": [
        "sharp"
      ]
    }
  }
}
